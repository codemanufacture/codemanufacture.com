{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "array-type": [
      true, "array"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "no-console": [
      false
    ],
    "no-var-requires": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "no-implicit-dependencies": [true, "dev"]
  },
  "rulesDirectory": []
}
