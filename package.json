{
  "name": "codemanufacture-website",
  "description": "Codemanufacture Website",
  "version": "0.1.0",
  "author": "Piotr Kwiecinski <piotr.kwiecinski@codemanufacure.com>",
  "private": true,
  "dependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.18",
    "gatsby-plugin-canonical-urls": "^2.0.6",
    "gatsby-plugin-catch-links": "^2.0.5",
    "gatsby-plugin-google-tagmanager": "^2.0.0",
    "gatsby-plugin-mailchimp": "https://github.com/benjaminhoffman/gatsby-plugin-mailchimp.git#gatsby-v2",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sitemap": "^2.0.0",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-remark-autolink-headers": "^2.0.8",
    "gatsby-remark-prismjs": "^3.0.3",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-json": "^2.1.4",
    "gatsby-transformer-remark": "^2.1.1",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.0.0",
    "throttle-debounce": "^2.0.1",
    "ts-loader": "^5.0.0",
    "typescript": "^3.1.3",
    "validator": "^10.9.0"
  },
  "homepage": "https://codemanufacture.com/",
  "keywords": [
    "gatsby"
  ],
  "license": "Apache-2.0",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn run:parallel lint:* nit:*",
    "develop": "gatsby develop -H 0.0.0.0",
    "format:markdown": "prettier --config .prettierrc --parser markdown --write '{./content/**/*.md,./README.md}'",
    "format:source": "prettier --config .prettierrc --parser typescript --write 'src/**/*.{ts,tsx}'",
    "graphql-types": "gql-gen",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:source": "tslint './src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --config .prettierrc --parser markdown --list-different '{./content/**/*.md,./README.md}'",
    "nit:source": "prettier --config .prettierrc --parser typescript --list-different 'src/**/*.{ts,tsx}'",
    "plop": "plop",
    "run:parallel": "npm-run-all --parallel",
    "serve": "gatsby serve",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "type-check": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "@types/react": "^16.4.2",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.6",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.0.1",
    "@types/throttle-debounce": "^1.1.0",
    "@types/uniqid": "^4.1.2",
    "@types/validator": "^9.4.3",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "graphql-code-generator": "^0.15.0",
    "graphql-codegen-add": "^0.15.0",
    "graphql-codegen-typescript-client": "^0.15.0",
    "graphql-codegen-typescript-common": "^0.14.5",
    "graphql-codegen-typescript-server": "^0.14.5",
    "husky": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "plop": "^2.0.0",
    "prettier": "1.15.3",
    "react-test-renderer": "^16.5.2",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0"
  }
}
