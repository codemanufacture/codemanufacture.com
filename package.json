  "homepage": "https://codemanufacture.com/",
  "keywords": [
    "gatsby"
  ],
  "license": "Apache-2.0",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn run:parallel lint:* nit:*",
    "clean": "gatsby clean",
    "develop": "gatsby develop -H 0.0.0.0",
    "format:markdown": "prettier --config .prettierrc --parser markdown --write '{./content/**/*.md,./README.md}'",
    "format:source": "prettier --config .prettierrc --parser typescript --write 'src/**/*.{ts,tsx}'",
    "graphql-types": "gql-gen",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:source": "tslint './src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --config .prettierrc --parser markdown --list-different '{./content/**/*.md,./README.md}'",
    "nit:source": "prettier --config .prettierrc --parser typescript --list-different 'src/**/*.{ts,tsx}'",
    "plop": "plop",
    "run:parallel": "npm-run-all --parallel",
    "serve": "gatsby serve",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "type-check": "tsc"
  },
  "resolutions": {
    "terser": "3.14.*"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@graphql-codegen/add": "^1.3.1",
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/typescript": "^1.3.1",
    "@graphql-codegen/typescript-operations": "^1.3.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.14.12",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-share": "^2.1.1",
    "@types/react-test-renderer": "^16.8.2",
    "@types/styled-components": "^4.1.16",
    "@types/throttle-debounce": "^1.1.1",
    "@types/uniqid": "^4.1.3",
    "@types/validator": "^10.11.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": ">=24.0.0 <25.0.0",
    "husky": "^2.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": ">=24.0.0 <25.0.0",
    "jest-haste-map": "^24.8.1",
    "jest-resolve": "^24.8.0",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "plop": "^2.4.0",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.8.6",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
